{
    "app-deployment": {
        "nodes": [
            {
                "type": "cache",
                "create-node-parameters": {
                    "name": "cache",
                    "deploy-command": "./client-name/cache/deploy.sh"
                }
            },
            {
                "type": "httpd",
                "create-node-parameters": {
                    "name": "http-static",
                    "deploy-command": "./client-name/http-static/deploy.sh"
                }
            },
            {
                "type": "httpd",
                "create-node-parameters": {
                    "name": "http-dynamic",
                    "deploy-command": "./client-name/http-dynamic/deploy.sh"
                }
            }
        ]
    }
}
