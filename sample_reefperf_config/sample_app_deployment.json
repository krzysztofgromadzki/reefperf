{
    "app-deployment": {
        "nodes": [
            {
                "type": "cache",
                "create-node-parameters": {
                    "name": {
                        "value": "cache"
                    },
                    "deploy-command": {
                        "value": "./client-name/cache/deploy.sh"
                    }
                }
            },
            {
                "type": "httpd",
                "create-node-parameters": {
                    "name": {
                        "value": "http-static"
                    },
                    "deploy-command": {
                        "value": "./client-name/http-static/deploy.sh"
                    }
                }
            },
            {
                "type": "httpd",
                "name": {
                    "value": "http-dynamic"
                },
                "deploy-command": {
                    "value": "./client-name/http-dynamic/deploy.sh"
                }
            }
        ]
    }
}
